{
  "version": "0.2.0",
  "configurations": [
    {
        "type": "node",
        "request": "launch",
        "name": "ts client: jest",
        "localRoot": "${workspaceFolder}/components/client/typescript",
        "program": "${workspaceFolder}/components/client/typescript/node_modules/jest/bin/jest",
        "args": [
          "--runInBand",
          "--no-cache",
        ],
        "outputCapture": "std",
        "console": "integratedTerminal",
      },
  ]
}
